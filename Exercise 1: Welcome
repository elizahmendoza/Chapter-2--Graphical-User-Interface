from tkinter import *
  
root = Tk() 
root.geometry("300x200")
root.resizable(width=False, height=False)
root.configure(bg='pink')
  
w = Label(text = "Welcome to My Page", font = ('Times', 18, 'bold')) 
w.pack() 
    
msg = Message( root, text = "This is Exercise 1")   
    
msg.pack()   
  
root.mainloop()  
